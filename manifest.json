{
  "manifest_version": 3,
  "name": "Chromo Timer",
  "version": "1.0.0",
  "description": "An extension with timer used on Chrome",
  "icons": {
    "16": "time-7-16.png",
    "32": "time-7-32.png",
    "48": "time-7-48.png",
    "128": "time-7-128.png"
  },
  "action": {
    "default_icon": {
      "16": "time-7-16.png",
      "32": "time-7-32.png",
      "48": "time-7-48.png",
      "128": "time-7-128.png"
    }
  },
  "permissions": ["storage", "contextMenus", "alarms", "notifications"],
  "background": {
  "service_worker": "service_worker.js"
  }
}
